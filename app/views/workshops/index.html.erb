<div class="projects-4" id="projects-4">
  <div class="container">
    <% if user_signed_in? %>
    <%= render 'workshops/member_index' %>
    <% else %>
    <%= render 'workshops/guest_index' %>
    <% end %>

    <div class="row">
      <% @workshops.drop(1).take(9).each do |workshop| %>

        <div class="col-md-4">
          <%= raw workshop.widget_embed %>
        </div>
      <% end %>
    </div>
  </div>
</div>
