html +='</div>';
var bw_container = $('#bigmarker-conference-widget-container7de7ace647b1a');
bw_container.addClass('bw-conference bigmarker-conference-widget-container').html(html);

add_theme(bw_container);
//bw_container.css('min-width', '240px');
//bw_container.css('width', '480px');
$('.bw-photo img', bw_container).css('max-width', '100%');

    });
  }

})(); // We call our anonymous function immediately

</script>